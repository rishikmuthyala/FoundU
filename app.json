{
  "expo": {
    "name": "FoundU.code",
    "slug": "FoundU_code",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "entryPoint": "./splash",
    "scheme": "foundu-code",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow UCard Finder to access your camera to take pictures of UCards.",
        "NSPhotoLibraryUsageDescription": "Allow UCard Finder to access your photos to select UCard images."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
        },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
        ]
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow UCard Finder to access your camera to take pictures of UCards."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
